<tr class="Uploading">
    <td class="Filetype ooo12g">
        <i class="ooofo {{ Fileicon }}"></i>
        <div class="ProgressContainer">
            <span class="Progress"></span>
        </div>
    </td>
    <td class="Filename ooo12g">{{ Filename }}</td>
    <td class="Filesize ooo12g"></td>
    <td class="Center">
        <a href="#" class="AttachmentDelete Hidden ooo12g" tabindex="0" aria-label="{{ 'Click to delete this attachment.' | Translate }}"><i class="ooofo ooofo-delete_outl"></i></a>
    </td>
</tr>
